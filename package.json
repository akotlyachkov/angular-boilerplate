{
    "name": "angular2-webpack2",
    "version": "2.0.0",
    "description": "angular4 universal sample app build with webpack3",
    "main": "start.js",
    "scripts": {
        "gulp": "gulp",
        "gulp watch": "gulp watch",
        "ngc browser": "ngc -p tsconfig.browser.ngc.json",
        "ngc server": "ngc -p tsconfig.server.ngc.json",
        "tsc browser aot": "tsc -p tsconfig.browser.aot.json",
        "tsc server aot": "tsc -p tsconfig.server.aot.json",
        "tsc browser": "tsc -p tsconfig.browser.json",
        "tsc server": "tsc -p tsconfig.server.json",
        "webpack browser": "webpack --config webpack.browser.js",
        "webpack browser watch": "webpack --config webpack.browser.js --watch",
        "webpack server": "webpack --config webpack.server.js",
        "webpack aot": "webpack --config webpack.server.aot.js && webpack --config webpack.browser.aot.js",
        "all server": "tsc -p tsconfig.server.json && webpack --config webpack.server.js",
        "rollup browser": "rollup --config rollup.browser.js",
        "rollup browser aot": "rollup --config rollup.browser.aot.js",
        "rollup server": "rollup --config rollup.server.js",
        "rollup server aot": "rollup --config rollup.server.aot.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/akotlyachkov/angular2-webpack2.git"
    },
    "keywords": [
        "angular",
        "angular4",
        "express",
        "webpack",
        "webpack3",
        "sample"
    ],
    "author": "akotlyachkov@gmail.com",
    "bugs": {
        "url": "https://github.com/akotlyachkov/angular2-webpack2/issues"
    },
    "homepage": "https://github.com/akotlyachkov/angular2-webpack2#readme",
    "dependencies": {
        "@angular/animations": "^4.3.3",
        "@angular/common": "^4.3.3",
        "@angular/compiler": "^4.3.3",
        "@angular/compiler-cli": "^4.3.3",
        "@angular/core": "^4.3.3",
        "@angular/forms": "^4.3.3",
        "@angular/http": "^4.3.3",
        "@angular/platform-browser": "^4.3.3",
        "@angular/platform-browser-dynamic": "^4.3.3",
        "@angular/platform-server": "^4.3.3",
        "@angular/router": "^4.3.3",
        "@nguniversal/express-engine": "^1.0.0-beta.2",
        "@types/express": "^4.0.36",
        "body-parser": "^1.17.2",
        "bootstrap": "^4.0.0-alpha.6",
        "compression": "^1.7.0",
        "core-js": "^2.5.0",
        "express": "^4.15.4",
        "font-awesome": "github:fortawesome/Font-Awesome",
        "helmet": "^3.8.1",
        "node-sass": "^4.5.3",
        "reflect-metadata": "^0.1.10",
        "rxjs": "^5.4.2",
        "zone.js": "^0.8.16"
    },
    "devDependencies": {
        "@ngtools/webpack": "^1.5.5",
        "@types/core-js": "^0.9.42",
        "@types/express": "^4.0.36",
        "@types/node": "^8.0.19",
        "angular2-template-loader": "^0.6.2",
        "awesome-typescript-loader": "^3.2.2",
        "base64-font-loader": "0.0.4",
        "bootstrap-sass": "^3.3.7",
        "css-loader": "^0.28.4",
        "del": "^3.0.0",
        "file-loader": "^0.11.2",
        "gulp": "^3.9.1",
        "gulp-concat": "^2.6.1",
        "gulp-concat-css": "^2.3.0",
        "gulp-inject": "^4.2.0",
        "gulp-sass": "^3.1.0",
        "gulp-sourcemaps": "^2.6.0",
        "html-loader": "^0.5.0",
        "node-sass": "^4.5.3",
        "raw-loader": "^0.5.1",
        "rollup-plugin-angular": "^0.4.8",
        "rollup-plugin-commonjs": "^8.1.0",
        "rollup-plugin-node-resolve": "^3.0.0",
        "rxjs-es": "^5.0.0-beta.12",
        "sass-loader": "^6.0.6",
        "source-map-loader": "^0.2.1",
        "style-loader": "^0.18.2",
        "typescript": "^2.4.2",
        "webpack": "^3.5.2"
    }
}
